{
  "name": "vue-split-grid",
  "version": "0.2.3",
  "description": "A Vue component based on Split Grid",
  "main": "dist/vue-scroll-table.umd.js",
  "module": "dist/vue-split-grid.common.js",
  "unpkg": "dist/vue-split-grid.umd.min.js",
  "browser": {
    "./SplitGrid": "src/components/SplitGrid.vue",
    "./SplitGridArea": "src/components/SplitGridArea.vue",
    "./SplitGridGutter": "src/components/SplitGridGutter.vue"
  },
  "scripts": {
    "serve": "vue serve --open examples/main.js",
    "lint": "vue-cli-service lint",
    "prepublishOnly": "npm run build:vue",
    "build:vue": "vue-cli-service build --target lib --name vue-split-grid ./src/wrapper.js",
    "build:rollup": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/vue-split-grid.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/vue-split-grid.esm.js",
    "build:unpkg": "cross-env SB_MINIFY=true rollup --config build/rollup.config.js --format iife --file dist/vue-split-grid.min.js"
  },
  "dependencies": {
    "split-grid": "^1.0.9"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-service": "^3.5.0",
    "babel-eslint": "^10.0.1",
    "cross-env": "^5.2.0",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.9.0",
    "rollup": "^1.6.0",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-uglify": "^6.0.2",
    "rollup-plugin-vue": "^4.7.2",
    "sass-loader": "^7.1.0",
    "vue": "^2.6.8",
    "vue-template-compiler": "^2.5.21"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stijlbreuk/vue-split-grid.git"
  },
  "keywords": [
    "vue",
    "split",
    "grid",
    "split.js",
    "split",
    "panel"
  ],
  "author": "Stijlbreuk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stijlbreuk/vue-split-grid/issues"
  },
  "homepage": "https://github.com/stijlbreuk/vue-split-grid#readme"
}
